//XXXXXXXXXXXXXXXXXXXXXXXXXXXX GANG KILL REACTION AND RESPECT XXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
//XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

SET_GANG_KILL_REACTION (redngang, zaibgang, 1)
SET_GANG_KILL_REACTION (sciegang, redngang, 1)
SET_GANG_KILL_REACTION (zaibgang, sciegang, 1)

CHANGE_GANG_CHAR_RESPECT (poligang, p1, -5)          //  They hate the player

DISABLE_CRANE (crane8)


//LAUNCH_MISSION (ste_zm3.mis)          //  (use this if you want to test a mission right away)
//ADD_SCORE (p1, 50000)                 //  (use this if you want to have money to save in the very start)


//XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
//XXXXXXXXXXXXXXXXXXXXXXXXXXXXXX MAIN LOOP XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
//XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX


WHILE ( true = 1 )
   
   //  If the redneck guy killed the scientist guy
   IF ( ( IS_CHAR_OBJECTIVE_PASSED(redneck_guy) ) AND ( has_redn_guy_objective_passed = 0 ) )
      MAKE_CHAR_DO_NOTHING (redneck_guy)
      SET_CHAR_SHOOTING_SKILL (redneck_guy, CRAP_SHOT)
      SET_CHAR_THREAT_REACTION (redneck_guy, REACT_AS_NORMAL)
      SET has_redn_guy_objective_passed = 1
	  
	  //  If the zaibatsu guy has killed the redneck guy
	  IF ( IS_CHAR_OBJECTIVE_PASSED(zaibatsu_guy) )
         MAKE_CHAR_DO_NOTHING (zaibatsu_guy)
      ENDIF
	  
   ENDIF
   
   
   
   //  Player has completed all Redneck missions
   IF ( ( flag_redn_missions_passed = 7 ) AND ( flag_all_redn_missions_completed = 0 ) )
      DISPLAY_MESSAGE (6185)      //  REDNECK GANG COMPLETE!
      DISPLAY_BRIEF (6182)        //  "Jesus H Christ, Rooster! You've passed all of the Redneck jobs."
      SET flag_all_redn_missions_completed = 1
   ENDIF
   
   //  Player has completed all Scientist missions
   IF ( ( flag_scie_missions_passed = 7 ) AND ( flag_all_scie_missions_completed = 0 ) )
      DISPLAY_MESSAGE (6184)      //  SCIENTIST GANG COMPLETE!
      DISPLAY_BRIEF (6183)        //  "Excellent work, THC-303! You have passed all of the Scientist jobs."
      SET flag_all_scie_missions_completed = 1
   ENDIF
   
   //  Player has completed all Zaibatsu missions
   IF ( ( flag_zaib_missions_passed = 7 ) AND ( flag_all_zaib_missions_completed = 0 ) )
      DISPLAY_MESSAGE (6186)      //  ZAIBATSU GANG COMPLETE!
      DISPLAY_BRIEF (6181)        //  "Well done, Gecko! You have passed all of the Zaibatsu jobs."
      SET flag_all_zaib_missions_completed = 1
   ENDIF
   
   //   It is causing issues, sometimes causing the end job to start twice at once
   /*
   IF ( ( flag_all_missions_passed = 21 ) AND ( flag_on_mission = 0 ) )
      SET flag_all_missions_passed = 22
      LAUNCH_MISSION (ste_end.mis)
   ENDIF
   */
   
   //  If the player has the final score and isn't playing a mission
   IF ( ( CHECK_SCORE_GREATER(p1, 2999999) ) AND ( flag_on_mission = 0 ) )
      IF ( can_player_finish_level = 0 )
         LEVEL_END_POINT_ARROW_AT (arrow4, 116.00, 123.00, 2.00)
         SET can_player_finish_level = 1
         DISPLAY_BRIEF_NOW (2910)          //  "HEY! You now have enough cash to leave this District. Follow the marker if you want to leave!"
      ENDIF
   ENDIF
   
   //  Is Player in mission and he got the final score?
   IF ( ( can_player_finish_level = 1 ) AND ( flag_on_mission = 1 ) )
      IF ( CHECK_SCORE_GREATER(p1, 2999999) )
         REMOVE_ARROW (arrow4)             //  Remove the end arrow
         SET can_player_finish_level = 0
      ENDIF
   ENDIF
   
   //  Player does not have 2999999 score anymore?
   IF ( ( can_player_finish_level = 1 ) AND ( flag_on_mission = 0 ) )
      IF ( NOT ( CHECK_SCORE_GREATER(p1, 2999999) ) )
         REMOVE_ARROW (arrow4)             //  Remove the end arrow
         SET can_player_finish_level = 0
         DISPLAY_BRIEF_NOW (2911)          //  "You don't have enough money to leave this District. You need $3,000,000!"
      ENDIF
   ENDIF
   
   //  If the player has the final score and he is in the end district point
   IF ( ( can_player_finish_level = 1 ) AND ( flag_on_mission = 0 ) )
      IF ( LOCATE_CHARACTER_ANY_MEANS(p1, 116.00, 123.00, 2.00, 1.00, 1.00) )
         FINISH_LEVEL (BONUS_1)            //  Finish Residential District
      ENDIF
   ENDIF

ENDWHILE

LEVELEND


