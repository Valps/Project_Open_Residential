	DISABLE_THREAD_TRIGGER (thr_wang_car_1)
 
 //  If the player didn't get the 8 wang cars yet and it is the first time he get into Wang Car 1
 IF ( ( wang_cars_count < 7 ) AND ( flag_wang_car_1 = 0 ) )
 
	   WARP_FROM_CAR_TO_POINT (p1, 200.50, 125.50, 2.00, 0)
	   ++wang_cars_count
    ADD_NEW_BLOCK (200, 125, 2)
	   CHANGE_BLOCK SIDE (200, 125, 2) BOTTOM NOT_WALL NOT_BULLET NOT_FLAT NOT_FLIP 0 791
